#include<stdio.h>
#include<stdlib.h>
#include<math.h>

int main () {
    float num1,num2;
    printf("Digite o primeiro numero: ");
    scanf("%f",&num1);
    printf("Digite o segundo numero: ");
    scanf("%f",&num2);
    printf("Soma: %.2f\n",(num1+num2));
    printf("Subtracao: %.2f\n",(num1-num2));
    printf("Multiplicacao: %.2f\n",(num1*num2));
    printf("Divisao: %.2f\n",(num1/num2));

    return 0;
}
